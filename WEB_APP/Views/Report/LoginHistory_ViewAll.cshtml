@model IEnumerable<CORPORATE_DISBURSEMENT_ADMIN_DAL.Models.ReportModels.LoginHistoryReport>
<table class="table table-bordered table-hover table-condensed table-sm" id="loginhistoryTable">
    <thead class="thead-light">
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.FullName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Role)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LoginTime)
            </th>           
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {            
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FullName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UserName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Role)
                </td>

                <td>
                    @item.LoginTime.ToString("yyyy-MM-dd hh:mm:ss tt")
                </td>                
            </tr>
        }
    </tbody>
</table>