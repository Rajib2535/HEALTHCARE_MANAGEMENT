@model IEnumerable<CORPORATE_DISBURSEMENT_ADMIN_DAL.Models.ReportModels.UserRegistrationSummary>
<table class="table table-bordered table-hover table-condensed table-sm" id="userRegistrationSummaryTable">
    <thead class="thead-light">
        <tr>

            <th>
                Full Name
            </th>

            <th>
               User Name
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>

            <th>
                Registration Time
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {            
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FullName)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.UserName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Status)
                </td>

                <td>
                    @(
                         item.RegistrationTime.ToString("yyyy-MM-dd")
                        )
                </td>
            </tr>
        }
    </tbody>
</table>